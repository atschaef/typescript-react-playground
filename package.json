{
  "name": "typescript-react-playground",
  "private": false,
  "engines": {
    "node": "^10",
    "npm": "^5.6"
  },
  "scripts": {
    "test": "BABEL_ENV=karma karma start ./build/karma.conf.js",
    "test:watch": "BABEL_ENV=karma karma start ./build/karma.conf.js --auto-watch --no-single-run",
    "storybook": "NODE_ENV=development start-storybook -p 6006",
    "build-storybook": "NODE_ENV=production build-storybook",
    "lint": "tslint -p ./tsconfig.test.json -c tslint.json",
    "types:check": "tsc -p ./tsconfig.test.json --noEmit",
    "precommit": "precise-commits",
    "make-me-pretty": "prettier --write {src/**,test/**,.}/*.{ts,js,tsx,jsx,json,md}"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "dependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.0.10",
    "@storybook/addon-info": "^4.0.10",
    "@storybook/addon-knobs": "^4.0.0-alpha.22",
    "@storybook/addon-links": "^4.0.0-alpha.22",
    "@storybook/cli": "^4.0.10",
    "@storybook/react": "^4.0.10",
    "@types/lodash-es": "^4.17.1",
    "@types/moment": "^2.13.0",
    "@types/react": "^16.7.11",
    "@types/react-dom": "^16.0.11",
    "@types/styled-components": "^4.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-jsx-control-statements": "^4.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-styled-components": "^1.9.2",
    "fork-ts-checker-notifier-webpack-plugin": "^0.7.0",
    "fork-ts-checker-webpack-plugin": "^0.5.0",
    "jsx-control-statements": "^3.2.8",
    "lodash-es": "^4.17.11",
    "moment": "^2.22.2",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "rimraf": "^2.6.2",
    "styled-components": "^4.1.2",
    "ts-loader": "^5.3.1",
    "typescript": "^3.2.1",
    "webpack": "^4.26.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/mocha": "^5.2.5",
    "@types/sinon": "^5.0.7",
    "chai": "^4.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^3.1.3",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "mocha": "^5.2.0",
    "precise-commits": "^1.0.2",
    "prettier": "^1.15.3",
    "sinon": "^7.1.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0"
  }
}
